EXPORT=SERVER.LISTEN:LISTEN
EXPORT=SPLITROUTES.IN:ROUTES
EXPORT=ROUTER.OUT:OUT

',' -> DELIMITER SplitRoutes(SplitStr)
SplitRoutes() OUT -> ROUTES Router(routers/RegexpRouter)

Server(webserver/Server) REQUEST -> IN ParseBody(webserver/BodyParser) OUT -> IN WebserverAdapter(woute/WebserverAdapter)
WebserverAdapter() RESPONSE -> IN Response(swiss/Trash)
WebserverAdapter() URL -> IN UrlSplit(Split)

MergeSections(packets/MergeConnections)

'session-id' -> GROUP SessionId(Group)
WebserverAdapter() SID -> IN SessionId() OUT -> IN MergeSections()
'url' -> GROUP Url(Group)
UrlSplit() OUT -> IN Url() OUT -> IN MergeSections()
'headers' -> GROUP Headers(Group)
WebserverAdapter() HEADERS -> IN Headers() OUT -> IN MergeSections()
'body' -> GROUP Body(Group)
WebserverAdapter() BODY -> IN Body() OUT -> IN MergeSections()

UrlSplit() OUT -> IN StringifyUrl(woute/StringifyUrl) OUT -> GROUP GroupConnection(groups/Group)
MergeSections() OUT -> IN GroupConnection() OUT -> IN Router()
